{

"description": "Best debate cardcutter for firefox east (and west) of the mississippi",
"manifest_version": 2,
"name": "Bosnian Cutter",
"version": "0.1",
"homepage_url": "https://www.github.com/barbraredpanda/bosnian-cutter",
"icons": {
    "48": "icons/mrbeast.jpg"
},

"content_scripts": [
    {
        "matches": ["<all_urls>"]
    }
],

"permissions":    [
    "activeTab",
    "tabs"
],

"browser_action": {
    "default_icon": "icons/mrbeast.jpg",
    "default_title": "MrBeasty",
    "default_popup": "popup/popup.html",
    "browser_style": true
},

"web_accessible_resources": [
]

  

}